{
    "name": "dungphanxuan/yii2-starter",
    "description": "Yii 2 Advanced Project Template",
    "keywords": ["yii2", "yii2 starter", "framework", "advanced", "project template"],
    "authors": [
        {
            "name": "Dung Phan Xuan",
            "email": "dungpx.s@gmail.com",
            "homepage": "http://github.com/dungphanxuan",
            "role": "Developer"
        }
    ],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/dungphanxuan/yii2-starter/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/dunghanxuan/yii2-startr"
    },
    "require": {
        "php": ">=5.5.0",
        "ext-intl": "*",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-authclient": "^2.0.0",
        "yiisoft/yii2-jui": "^2.0.0",
        "yiisoft/yii2-redis": "^2.0",
        "asofter/yii2-imperavi-redactor": "dev-master@dev",
        "mihaildev/yii2-elfinder": "^1.0",
        "trntv/yii2-aceeditor": "^2.0",
        "trntv/yii2-file-kit": "^1.0.0",
        "trntv/yii2-glide": "^1.0.0",
        "trntv/yii2-datetime-widget": "^1.0.0",
        "trntv/cheatsheet": "^0.1@dev",
        "facebook/php-sdk-v4": "^5.1",
        "miloschuman/yii2-highcharts-widget": "dev-master",
        "sunra/php-simple-html-dom-parser": "^1.5",
        "froala/yii2-froala-editor": "dev-master",
        "bower-asset/admin-lte": "^2.0",
        "bower-asset/font-awesome": "^4.0",
        "bower-asset/html5shiv": "^3.0",
        "bower-asset/jquery-slimscroll": "^1.3",
        "symfony/process": "^3.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    }
}
